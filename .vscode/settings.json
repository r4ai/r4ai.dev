{
  "tailwindCSS.experimental.classRegex": [
    // key名がtwで始まるものをtailwindのクラスとして認識する
    // e.g. twColor: text-red-500;
    // > https://github.com/tailwindlabs/tailwindcss-intellisense/issues/270
    "tw.*:(?:.|\n)*?[\"'`]([^\"'`]*).*?,"
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "mdx.server.enable": true
}
